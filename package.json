{
  "name": "rise-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "playwright:install": "pnpm exec playwright install",
    "playwright:local": "pnpm exec playwright test --project='chromium'",
    "playwright:test": "pnpm exec playwright test",
    "playwright:dev": "export IS_PLAYWRIGHT=true && next dev -p 4000",
    "playwright:start": "export IS_PLAYWRIGHT=true && next start -p 4000",
    "playwright:build": "export IS_PLAYWRIGHT=true && next build",
    "playwright:ci": "pnpm playwright:build && pnpm playwright:test",
    "prettier": "pnpm exec prettier . --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "2.1.1",
    "@next/env": "^14.2.7",
    "@sanity/image-url": "1.0.2",
    "@sanity/vision": "3.53.0",
    "classnames": "^2.5.1",
    "next": "14.2.5",
    "next-sanity": "9.4.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "sanity": "^3.53.0",
    "styled-components": "^6.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@playwright/test": "^1.46.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "globals": "^15.10.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.9",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "typescript-eslint": "^8.8.0"
  }
}
